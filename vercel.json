{
    "version": 2,
    "builds": [
      {
        "src": "frontend/src/app.tsx",
        "use": "@vercel/vite"
      },
      {
        "src": "backend/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/src/server.ts"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/src/App.tsx"
      }
    ]
  }
  